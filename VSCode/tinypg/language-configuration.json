	"comments": {
		"lineComment": "//",
		"blockComment": [ "/*", "*/" ]
	},
	"brackets": [
		["{", "}"]
	],
	"autoClosingPairs": [
		["\"", "\""],
		["@\"", "\""],
		["(", ")"],
		["{", "}"]
	],
	"surroundingPairs": [
		["\"", "\""],
		["@\"", "\""],
		["(", ")"],
		["{", "}"]
	],
	"folding": {
		"markers": {
